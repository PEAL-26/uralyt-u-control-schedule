{
    "cli": {
      "version": ">= 3.18.3",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "channel": "development",
        "developmentClient": true,
        "distribution": "internal",
        "env": {
          "NODE_ENV": "development"
        },
        "android": {
          "image": "latest"
        },
        "ios": {
          "image": "latest"
        }
      },
      "preview": {
        "channel": "preview",
        "android": {
          "buildType": "apk",
          "image": "latest"
        },
        "ios": {
          "image": "latest"
        },
        "env": {
          "APP_ENV": "development",
          "NODE_ENV": "development"
        }
      },
      "production": {
        "autoIncrement": true,
        "channel": "production",
        "env": {
          "APP_ENV": "production",
          "NODE_ENV": "production"
        },
        "android": {
          "image": "latest"
        },
        "ios": {
          "image": "latest"
        }
      }
    },
     "submit": {
      "production": {
        "android": {
          "track": "production"
        },
        "ios": {
          "appleId": "123",
          "ascAppId": "123",
          "appleTeamId": "1234",
          "language": "pt-BR",
          "companyName": "peal26"
        }
      }
    }
  }